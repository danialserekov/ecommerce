<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@drawable/bell" android:id="@+id/imageView"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp"/>

    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@drawable/user" android:id="@+id/imageView2"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/imageView4"/>

    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@drawable/home" android:id="@+id/imageView3"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintEnd_toStartOf="@+id/imageView5"
            android:layout_marginEnd="32dp"/>

    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@drawable/love" android:id="@+id/imageView4"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/imageView5" app:layout_constraintEnd_toStartOf="@+id/imageView2"
            android:layout_marginEnd="32dp"/>

    <ImageView
            android:layout_width="36dp"
            android:layout_height="36dp" app:srcCompat="@drawable/trolley" android:id="@+id/imageView5"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/imageView3" app:layout_constraintEnd_toStartOf="@+id/imageView4"
            android:layout_marginEnd="32dp"/>

    <TextView
            android:text="DevBook Shop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView"
            android:textColor="#039BE5"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="24dp"/>

    <EditText
            android:id="@+id/search_bar"
            android:layout_width="400dp"
            android:layout_height="48dp"
            android:hint="Search"
            android:textColor="@color/black"
            android:gravity="center"
            android:inputType="textPassword"
            android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginTop="16dp"/>
    <TextView
            android:text="My Wishlist"
            android:layout_width="wrap_content"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold"
            android:layout_height="wrap_content" android:id="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="200dp"
            android:layout_marginEnd="200dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/search_bar"/>

    <ImageView
            android:layout_width="180dp"
            android:layout_height="130dp" app:srcCompat="@drawable/mac" android:id="@+id/imageView6"
            tools:layout_editor_absoluteY="167dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="MacBook Pro M4 Pro"
            android:layout_width="wrap_content"
            android:textColor="@color/black"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            tools:layout_editor_absoluteY="167dp" app:layout_constraintStart_toEndOf="@+id/imageView6"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="950 000 KZT"
            android:layout_width="wrap_content"
            android:textColor="@color/black"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/imageView6" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"/>
    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@drawable/heart" android:id="@+id/imageView22"
            tools:layout_editor_absoluteY="167dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"/>

    <ImageView
            android:layout_width="180dp"
            android:layout_height="130dp"
            app:srcCompat="@drawable/watch"
            android:id="@+id/imageView17"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="316dp"/>
    <TextView
            android:text="Apple Watch Series 10"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView13" tools:layout_editor_absoluteY="318dp"
            android:layout_marginStart="16dp" app:layout_constraintStart_toEndOf="@+id/imageView17"/>
    <TextView
            android:text="250 000 KZT"
            android:textColor="@color/black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView14"
            android:layout_marginStart="16dp" app:layout_constraintStart_toEndOf="@+id/imageView17"
            app:layout_constraintTop_toBottomOf="@+id/textView13" android:layout_marginTop="8dp"/>
    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@drawable/heart" android:id="@+id/imageView27"
            tools:layout_editor_absoluteY="318dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="View product"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button" tools:layout_editor_absoluteY="398dp"
            tools:layout_editor_absoluteX="212dp"/>
    <Button
            android:text="View product"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button3" tools:layout_editor_absoluteY="249dp"
            tools:layout_editor_absoluteX="212dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>