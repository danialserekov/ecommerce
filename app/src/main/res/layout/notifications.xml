    <ImageView
            android:layout_width="36dp"
            android:layout_height="36dp" app:srcCompat="@drawable/trolley" android:id="@+id/imageView5"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/imageView3" app:layout_constraintEnd_toStartOf="@+id/imageView4"
            android:layout_marginEnd="32dp"/>
    <TextView
            android:text="Manage your Address"
            android:textSize="16dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView59" tools:layout_editor_absoluteY="94dp"
            app:layout_constraintStart_toEndOf="@+id/imageView47" android:layout_marginStart="16dp"/>
    <TextView
            android:text="Please take a moment to update your address information to ensure accurate and timely deliveries. Thank you for your attention!       "
            android:layout_width="282dp"
            android:layout_height="71dp" android:id="@+id/textView60"
            android:layout_marginStart="16dp" app:layout_constraintStart_toEndOf="@+id/imageView47"
            android:layout_marginTop="12dp" app:layout_constraintTop_toBottomOf="@+id/textView59"
    />
    <ImageView
            android:layout_width="64dp"
            android:layout_height="64dp" app:srcCompat="@drawable/location" android:id="@+id/imageView47"
            tools:layout_editor_absoluteY="105dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <ImageView
            android:layout_width="64dp"
            android:layout_height="64dp" app:srcCompat="@drawable/heart" android:id="@+id/imageView48"
            tools:layout_editor_absoluteY="242dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="Check your Wishlist"
            android:textSize="16dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView61"
            android:layout_marginStart="16dp" app:layout_constraintStart_toEndOf="@+id/imageView48"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/textView60"/>
    <TextView
            android:text="Check your Orders"
            android:textSize="16dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView64"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView62" android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/imageView49"/>
    <TextView
            android:text="Don't forget to review your wishlist for items you're interested in. It's a great way to keep track of your favorite products and make future purchases easier.  "
            android:layout_width="293dp"
            android:layout_height="70dp" android:id="@+id/textView62"
            android:layout_marginStart="16dp" app:layout_constraintStart_toEndOf="@+id/imageView48"
            app:layout_constraintTop_toBottomOf="@+id/textView61" android:layout_marginTop="12dp"/>
    <TextView
            android:text="Please review your recent orders to stay updated on their status. If you have any questions or concerns, feel free to reach out.       "
            android:layout_width="293dp"
            android:layout_height="60dp" android:id="@+id/textView63"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/textView64" android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/imageView49"/>
    <ImageView
            android:layout_width="64dp"
            android:layout_height="64dp" app:srcCompat="@drawable/sent" android:id="@+id/imageView49"
            tools:layout_editor_absoluteY="380dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>