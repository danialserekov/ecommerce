<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@drawable/user" android:id="@+id/imageView2"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/imageView4"/>

    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@drawable/home" android:id="@+id/imageView3"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintEnd_toStartOf="@+id/imageView5"
            android:layout_marginEnd="32dp"/>

    <ImageView
            android:layout_width="32dp"
            android:layout_height="32dp" app:srcCompat="@drawable/love" android:id="@+id/imageView4"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/imageView5" app:layout_constraintEnd_toStartOf="@+id/imageView2"
            android:layout_marginEnd="32dp"/>

    <ImageView
            android:layout_width="36dp"
            android:layout_height="36dp" app:srcCompat="@drawable/trolley" android:id="@+id/imageView5"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" android:layout_marginStart="32dp"
            app:layout_constraintStart_toEndOf="@+id/imageView3" app:layout_constraintEnd_toStartOf="@+id/imageView4"
            android:layout_marginEnd="32dp"/>

    <TextView
            android:text="Payment Success"
            android:textSize="24dp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView25"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="32dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="128dp" android:layout_marginEnd="128dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="344dp"
            android:layout_height="204dp" app:srcCompat="@drawable/how5" android:id="@+id/imageView39"
            tools:layout_editor_absoluteY="235dp" tools:layout_editor_absoluteX="33dp"/>
    <Button
            android:text="Go Back"
            android:layout_width="250dp"
            android:layout_height="40dp" android:id="@+id/button7" tools:layout_editor_absoluteY="460dp"
            tools:layout_editor_absoluteX="80dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>