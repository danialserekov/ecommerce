            android:layout_width="26dp"
            android:layout_height="26dp" app:srcCompat="@drawable/minus" android:id="@+id/imageView24"
            tools:layout_editor_absoluteY="154dp" tools:layout_editor_absoluteX="220dp"/>
    <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp" app:srcCompat="@drawable/plus" android:id="@+id/imageView23"
            tools:layout_editor_absoluteY="154dp"
            tools:layout_editor_absoluteX="278dp"/>
    <ImageView
            android:layout_width="26dp"
            android:layout_height="26dp" app:srcCompat="@drawable/square" android:id="@+id/imageView25"
            tools:layout_editor_absoluteX="249dp" tools:layout_editor_absoluteY="154dp"/>
    <TextView
            android:text="1"
            android:layout_width="8dp"
            android:textColor="@color/black"
            android:textSize="16dp"
            android:layout_height="23dp" android:id="@+id/textView46" tools:layout_editor_absoluteY="155dp"
            tools:layout_editor_absoluteX="258dp"/>


