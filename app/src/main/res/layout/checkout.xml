            android:text="953 500 KZT"
            android:textSize="18dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView45"
            android:layout_marginEnd="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView44"/>

    <TextView
            android:text="MacBook Pro M4 Pro"
            android:layout_width="wrap_content"
            android:textColor="@color/black"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            tools:layout_editor_absoluteY="94dp" app:layout_constraintStart_toEndOf="@+id/imageView6"
            android:layout_marginStart="16dp"/>
    <TextView
            android:text="950 000 KZT"
            android:layout_width="wrap_content"
            android:textColor="@color/black"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toEndOf="@+id/imageView6" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"/>
    <Button
            android:text="Proceed Payment"
            android:layout_width="324dp"
            android:layout_height="41dp" android:id="@+id/button5" tools:layout_editor_absoluteY="625dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="64dp"
            android:layout_marginEnd="64dp" app:layout_constraintEnd_toEndOf="parent"/>
    <Button
